# Auto detect text files and perform LF normalization
* text=auto
*.vtf filter=lfs diff=lfs merge=lfs -text
*.bsp filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
client.bin filter=lfs diff=lfs merge=lfs -text
server.bin filter=lfs diff=lfs merge=lfs -text
*.dx80.vtx filter=lfs diff=lfs merge=lfs -text
*.dx90.vtx filter=lfs diff=lfs merge=lfs -text
*.mdl filter=lfs diff=lfs merge=lfs -text
*.sw.vtx filter=lfs diff=lfs merge=lfs -text
*.vvd filter=lfs diff=lfs merge=lfs -text
